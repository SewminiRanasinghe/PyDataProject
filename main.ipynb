    "import pandas as pd\n",
    "import glob\n",
    "\n",
    "\n",
    "csv_files = glob.glob(\"WorldCup_Stats/*.csv\") \n",
    "dataframes = [pd.read_csv(file) for file in csv_files]\n",
    "\n",
    "\n",
    "crick_df = pd.concat(dataframes, ignore_index=True)\n",
